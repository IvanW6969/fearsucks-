-- Scripts:

local function KUHDRO_fake_script() -- Name_2.LocalScript 
	local script = Instance.new('LocalScript', Name_2)

	local UserName = game.Players.LocalPlayer.Name
	local Gui = script.Parent.Parent.Parent
	local Frame = script.Parent.Parent
	local Textlabel = script.Parent
	Textlabel.Text = "Username: "..UserName
	print("Username Loaded!")
end
coroutine.wrap(KUHDRO_fake_script)()
local function MBAH_fake_script() -- FPS.LocalScript 
	local script = Instance.new('LocalScript', FPS)

	local RS = game:GetService("RunService")
	local frames = 0
	RS.RenderStepped:Connect(function()
		frames = frames + 1
	end)
	while wait(1) do
		script.Parent.Text = "Current FPS: "..frames
		frames = 0
	end
end
coroutine.wrap(MBAH_fake_script)()
local function DLBP_fake_script() -- Cash.LocalScript 
	local script = Instance.new('LocalScript', Cash)
	while true do task.wait(0.1)
	local Pref = "$"
	local cash = game.Players.Labadie37956.DataFolder.Currency.Value
	local Textlbl = script.Parent
	Textlbl.Text = "Cash: $"..cash
end
coroutine.wrap(DLBP_fake_script)()
local function IXUNLQL_fake_script() -- betterfeargui.Drag 
	local script = Instance.new('LocalScript', betterfeargui)

	local Frame = script.Parent
	Frame.Active = true
	Frame.Draggable = true
end
coroutine.wrap(IXUNLQL_fake_script)()
local function DYUIJ_fake_script() -- FEARSUCKS.LocalScript 
	local script = Instance.new('LocalScript', FEARSUCKS)
	
	local Gui = script.Parent.Parent.Parent
	local Frame = script.Parent.Parent
	local Textlabel = script.Parent
	Textlabel.Text = "Fear Is The Shittest Scripter Ever"
	TextLabel.TextScaled = true
	print("Fear Sucks, Made by Tyronee#5559")
end
coroutine.wrap(DYUIJ_fake_script)()
